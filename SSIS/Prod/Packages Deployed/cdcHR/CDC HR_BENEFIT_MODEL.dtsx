                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT   paaf.person_id,
         paaf.assignment_id,
         paaf.effective_start_date AS assign_eff_start_dt,
         paaf.effective_end_date AS assign_eff_end_dt,
         pty.name AS plan_type,
         bpf.name AS plan_name,
         opt.name AS plan_option,
         (CASE
             WHEN rt.acty_typ_cd = 'EEPYC' THEN 'Employee Cost'
             WHEN rt.acty_typ_cd = 'ERPYC' THEN 'Employer Cost'
          END)
            AS emp_emplyr_cost,
         rt.rt_strt_dt AS rate_start_dt,
         rt.rt_end_dt AS rate_end_dt,
         rt.rt_val AS rate,
         rt.ann_rt_val AS annual_rate,
         enrt.effective_start_date AS enroll_eff_start_dt,
         enrt.effective_end_date AS enroll_eff_end_dt,
         enrt.enrt_cvg_strt_dt AS enroll_cvg_start_dt,
         enrt.enrt_cvg_thru_dt AS enroll_cvg_end_dt,
         bpf.pln_attribute1,
         bpf.pln_attribute2,
         ppb.pay_basis,
         ppb.name AS pay_basis_name, rt.acty_typ_cd, enrt.pen_attribute1 LTD_PREMIUM_OPTION   
  FROM               per_all_assignments_f paaf
                  INNER JOIN
                     per_pay_bases ppb
                  ON paaf.pay_basis_id = ppb.pay_basis_id
               INNER JOIN
                  hr_locations_all hl
               ON paaf.location_id = hl.location_id
            INNER JOIN
                           ben_oipl_f oipl
                        INNER JOIN
                           ben_prtt_enrt_rslt_f enrt
                        ON oipl.oipl_id = enrt.oipl_id
                        left outer join       
            ben_prtt_rt_val rt
         ON rt.prtt_enrt_rslt_id = enrt.prtt_enrt_rslt_id 
                     INNER JOIN
                        ben_opt_f opt
                     ON oipl.opt_id = opt.opt_id
                  INNER JOIN
                     ben_pl_f bpf
                  ON oipl.pl_id = bpf.pl_id AND enrt.pl_id = bpf.pl_id
               INNER JOIN
                  ben_pl_typ_f pty
               ON bpf.pl_typ_id = pty.pl_typ_id
            ON paaf.person_id = enrt.person_id
 WHERE  (rt.acty_typ_cd IN ('ERPYC', 'EEPYC') or rt.acty_typ_cd is null)</property>
