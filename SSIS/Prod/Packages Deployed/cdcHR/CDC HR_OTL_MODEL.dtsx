                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT ppf.person_id,
       paf.assignment_id,
       ppf.full_name,
       ppb.pay_basis,
       ppf.attribute1 employee_number,
       ppg.segment1 shift,
       petf.reporting_name work_center,
       petf.element_name,
       peef.date_earned,
       c.screen_entry_value hours,
       ppp.proposed_salary_n hourly_rate,
       (c.screen_entry_value * ppp.proposed_salary_n) amount_earned,
       ptp.start_date week_start,
       ptp.end_date week_end,
       c.effective_start_date biweek_start,
       c.effective_end_date biweek_end,
       hl.location_code
FROM pay_element_types_f petf,
     pay_element_links_f pelf,
     pay_element_entries_f peef,
     per_all_assignments_f paf,
     per_all_people_f ppf,
     pay_element_entry_values_f c,
     pay_input_values_f d,
     per_pay_proposals ppp,
     pay_people_groups ppg,
     per_time_periods ptp,
     per_person_types ppt,
     per_pay_bases ppb,
     hr_locations_all hl
WHERE     petf.element_type_id = pelf.element_type_id
      AND pelf.element_link_id = peef.element_link_id
      AND peef.assignment_id = paf.assignment_id
      AND ppp.assignment_id = paf.assignment_id
      AND ptp.start_date BETWEEN ppf.effective_start_date
                             AND  ppf.effective_end_date
      AND ptp.start_date BETWEEN paf.effective_start_date
                             AND  paf.effective_end_date
      AND paf.person_id = ppf.person_id
      AND ppt.person_type_id = ppf.person_type_id
      AND ppt.system_person_type = 'EMP'
      AND paf.pay_basis_id = ppb.pay_basis_id
      AND UPPER (ppb.pay_basis) = 'HOURLY'
      AND paf.people_group_id = ppg.people_group_id
      AND c.element_entry_id = peef.element_entry_id
      AND c.effective_start_date = peef.effective_start_date
      AND c.effective_end_date = peef.effective_end_date
      AND d.input_value_id = c.input_value_id
      AND d.name = 'Hours'
      AND ptp.period_type = 'Week'
      AND paf.location_id = hl.location_id
      AND peef.date_earned BETWEEN ptp.start_date AND ptp.end_date
      AND ptp.start_date &gt; ADD_MONTHS(LAST_DAY(trunc(sysdate)),-8)</property>
