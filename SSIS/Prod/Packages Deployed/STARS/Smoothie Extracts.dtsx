                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  [part]
	  , case 
		when plant_code = 'laa' and Planner_Code = 'PURSAMEX' Then 'CONASP' Else plant_code End plant_code
      , period
      ,sum(total_demand)													GrossReq
      ,	0																	BuildSchedule
      , 0																	OpenPOs
      , sum(receipt_forecast - (open_supply_receipts + past_due_supply))	PlannedOrd
      , sum(in_transit_receipts)											Inspection
      , 0																	Purchase_requisition_qty -- Can look further at prdrec but looks the sam as PlannedOrd

  from dbo.conmed_sp_future
  where 1=1
  and Not (
  total_demand = 0
  and   [past_due_supply] = 0
  and   [open_supply_receipts] = 0
  and   [receipt_forecast] = 0
  and  in_transit_receipts = 0

  )

  group by plant_code, planner_code, part, period

  order by plant_code, planner_code, part, period</property>
